{% extends 'base.html.twig' %}

{% block body %}

    <!-- Main title -->
    <h1 id="form_main_title">S'incrire</h1>

    <!-- Form setting and display -->
    <div id="form_container">

        <!-- Define the form theme -->
        {% form_theme form
            'form/_errors_form_theme.html.twig'
            'form/_image_form_theme.html.twig'
            'form/_register_form_theme.html.twig'
        %}

        <!-- Ask the form's view display -->
        {{ form(form) }}

    </div>

{% endblock %}
